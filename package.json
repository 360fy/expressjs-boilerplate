{
  "name": "expressjs-boilerplate",
  "version": "1.3.6",
  "description": "Boilerplate for expressjs based application",
  "repository": "git@github.com:360fy/expressjs-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && mkdir -p lib/server && cp -R src/server/views/ lib/server/views && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.0",
    "body-parser": "1.15.1",
    "cli-table2": "0.2.0",
    "command-line-boilerplate": "1.1.6",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "events": "1.1.0",
    "express": "4.13.4",
    "express-handlebars": "3.0.0",
    "express-winston": "1.4.1",
    "handlebars": "4.0.5",
    "immutable": "3.8.1",
    "lodash": "4.13.1",
    "ms": "0.7.1",
    "performance-now": "0.2.0",
    "qs": "6.2.0",
    "serve-favicon": "2.3.0",
    "socket.io": "1.4.6",
    "socket.io-client": "1.4.6",
    "winston": "2.2.0",
    "winston-daily-rotate-file": "1.1.1"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.5.0",
    "eslint": "2.11.1",
    "eslint-config-360fy": "1.1.9",
    "eslint-plugin-react": "5.1.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
