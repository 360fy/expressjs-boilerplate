{
  "name": "expressjs-boilerplate",
  "version": "1.0.7",
  "description": "Boilerplate for expressjs based application",
  "private": true,
  "repository": "git+ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/expressjs-boilerplate",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && mkdir -p lib/server && cp -R src/server/views/ lib/server/views && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.1.5",
    "body-parser": "~1.14.2",
    "cli-boilerplate": "https://private-npm-releases.s3.amazonaws.com/cli-boilerplate-1.0.0.tgz",
    "cli-table2": "^0.1.9",
    "compression": "^1.6.1",
    "config": "^1.19.0",
    "cookie-parser": "^1.4.1",
    "debug": "~2.2.0",
    "events": "^1.1.0",
    "express": "~4.13.4",
    "immutable": "^3.7.6",
    "jade": "~1.11.0",
    "joi": "^7.2.2",
    "jquery": "^2.2.0",
    "js-yaml": "^3.5.2",
    "lodash": "^4.0.1",
    "morgan": "~1.6.1",
    "ms": "^0.7.1",
    "performance-now": "^0.2.0",
    "qs": "^6.1.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^2.0.0-rc5",
    "reactjs-web-boilerplate": "https://private-npm-releases.s3.amazonaws.com/reactjs-web-boilerplate-1.0.5.tgz",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-preset-node5": "^10.5.0",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-360fy": "https://private-npm-releases.s3.amazonaws.com/eslint-config-360fy-1.0.0.tgz",
    "eslint-plugin-react": "^3.16.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "presets": [
      "node5",
      "react"
    ]
  }
}
