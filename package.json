{
  "name": "expressjs-boilerplate",
  "version": "1.1.0",
  "description": "Boilerplate for expressjs based application",
  "repository": "git@github.com:360fy/expressjs-boilerplate.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && mkdir -p lib/server && cp -R src/server/views/ lib/server/views && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.4",
    "body-parser": "~1.15.0",
    "cli-table2": "^0.2.0",
    "command-line-boilerplate": "^1.0.2",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "debug": "~2.2.0",
    "events": "^1.1.0",
    "express": "~4.13.4",
    "express-winston": "^1.3.0",
    "immutable": "^3.7.6",
    "jade": "~1.11.0",
    "jquery": "^2.2.1",
    "lodash": "^4.6.1",
    "ms": "^0.7.1",
    "performance-now": "^0.2.0",
    "qs": "^6.1.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-node5": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-360fy": "^1.0.1",
    "eslint-plugin-react": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
